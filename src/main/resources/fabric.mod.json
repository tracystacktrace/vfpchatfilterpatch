{
  "schemaVersion": 1,
  "id": "vfpchatfilterpatch",
  "version": "${version}",
  "name": "VFP ChatFilter Patch",
  "description": "A patch for the client-side chat filter of ViaLegacy library (ViaFabricPlus), allowing to manipulate and change its behaviour.",
  "authors": ["tracystacktrace"],
  "contact": {
    "issues": "https://github.com/tracystacktrace/vfpchatfilterpatch/issues",
    "sources": "https://github.com/tracystacktrace/vfpchatfilterpatch"
  },
  "license": "Apache-2.0",
  "icon": "assets/vfpchatfilterpatch/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "net.tracystacktrace.vfpchatfilterpatch.VFPChatFilterPatch"
    ]
  },
  "mixins": [
    {
      "config": "vfpchatfilterpatch.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "minecraft": "${minecraft_version}",
    "viafabricplus": ">=${viafabricplus_version}"
  }
}
